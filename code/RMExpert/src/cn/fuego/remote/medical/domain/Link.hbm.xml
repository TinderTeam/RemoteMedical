<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
	
<hibernate-mapping
	package="cn.fuego.remote.medical.domain">
	<class name="Link" table="link">

		<composite-id>
		   <key-property name="expertID" column="Es_ID" length="64" ></key-property>
		
	       <key-property name="hospitalID" column="Hs_ID" length="64" ></key-property>
	    </composite-id>
	    
	     <property name="expertName" column="Hs_Name" length="64" not-null="true"></property>
	    
	     <property name="hospitalName" column="Es_Name" length="64" not-null="true"></property>
	     <property name="linkTime" column="LinkTime"  ></property>
		 <property name="auditor" column="Auditor" length="64"></property>


		 <property name="linkState"  not-null="true">
		  <column name="LinkState" sql-type="smallint"></column>
		 </property>


	</class>
</hibernate-mapping>