<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
	
<hibernate-mapping
	package="cn.fuego.remote.medical.domain">
	<class name="Report" table="report">
		<composite-id>
		    <key-property name="serialNo" column="Pi_SerialNO" ></key-property>
	       <key-property name="hospitalID" column="Hs_ID" length="64" ></key-property>

	    </composite-id>
        <property name="reportNo" column="Rs_ReportNO" length="64" not-null = "true"></property>
        <property name="patientID" column="Rs_PatientID" length="64" ></property>
        <property name="patientName" column="Rs_PatientName" length="64" ></property>
        <property name="patientSex" column="Rs_PatientSex" length="4" ></property>
        <property name="patientAge" column="Rs_PatientAge" length="16" ></property>
        
        <property name="modality" column="Rs_Modality" length="16" ></property>
        
        <property name="department" column="Rs_Department" length="20"></property>
        <property name="hospitalNo" column="Rs_HospitalNO" length="10"></property>
        <property name="bedNo" column="Rs_BedNO" length="6"></property>
        <property name="bodyPart" column="Rs_BodyPart" length="64"></property>
        <property name="preView" column="Rs_PreView"></property>
        <property name="diagnosis" column="Rs_Diagnosis"></property>
        
        <property name="studyContent"  type="text" >
          <column name="Rs_StudyContent" sql-type="text"></column>
        </property>
        <property name="studyConclusion"  type="text" >
          <column name="Rs_StudyConclusion" sql-type="text"></column>
        </property>
        <property name="exStudyContent"  type="text" >
          <column name="Rs_ExStudyContent" sql-type="text"></column>
        </property>
        <property name="exStudyConclusion"  type="text" >
          <column name="Rs_ExStudyConclusion" sql-type="text"></column>
        </property>
 
        <property name="exStudyContentCode" column="Rs_ExStudyContentCode"  ></property>
        
        <property name="exStudyConclusionCode" column="Rs_ExStudyConclusionCode"></property>
        <property name="studyDate" column="Rd_StudyDate"></property>
        <property name="reportDate" column="Rd_ReportDate"></property>
        <property name="reviewDate" column="Rd_ReviewDate"></property>
        <property name="operatorsPhysician" column="Rs_OperatorPhysician"  length="64"></property>
        <property name="reportPhysician" column="Rs_ReportPhysician"  length="64"></property>
        <property name="reviewPhysician" column="Rs_ReviewPhysician"  length="64"></property>
        <property name="imageCount" >
            <column name="Ri_ImageCount" sql-type="smallint"></column> 
        </property>
        <property name="imagePathWL" column="Rs_ImagePathWL"></property>
        <property name="exApply" column="Rd_ExApply"></property>
        <property name="exStartUpImg" column="Rd_ExStartUpImg"></property>
        <property name="exEndUpImg" column="Rd_ExEndUpImg"></property>
        <property name="exGetReport" column="Rd_ExGetReport"></property>
        
        
        <property name="exReport" column="Rd_ExReport"></property>
        <property name="exDownReport" column="Rd_ExDownReport"></property>
        <property name="exApplyDoctor" column="Rs_ExApplyDoctor" length="64"></property>
        
        <property name="exDoctor" column="Rs_ExpertID" length="64"></property>
        <property name="expertDoctorName" column="Rs_ExpertDoctor" length="64"></property>
        
        <property name="exState" column="Ri_ExState" ></property>
        <property name="exReportState" column="Ri_ExReportState"></property>
        
        <property name="positive" column="Rs_Positive" ></property>
        
        <property name="reserve1" column="Rs_Reserve1" length="64"></property>
        <property name="reserve2" column="Rs_Reserve2" length="128"></property>
        <property name="reserve3" column="Rs_Reserve3" length="255"></property>
        <property name="reserve4" >
          <column name="Rs_Reserve4" sql-type="text"></column> 
        </property>
        <property name="reserve5" column="Ri_Reserve5"></property>
        <property name="reserve6"  >
            <column name="Ri_Reserve6" sql-type="smallint"></column> 
        </property>
                
        
	</class>
</hibernate-mapping>