		 <property name="mrCount" column="Ei_MRCount"></property>
		 <property name="ulCount" column="Ei_ULCount"></property>
		 <property name="otherCount" column="Ei_OtherCount"></property>	
		 <property name="reserve1" column="Es_Reserve1"></property>	 	 
	     <property name="reserve2" column="Es_Reserve2"></property>
		 <property name="reserve3" column="Es_Reserve3"></property>
		 <property name="reserve4" column="Ei_Reserve4"></property>
		 <property name="reserve5" column="Ei_Reserve5"></property>


	</class>
</hibernate-mapping>