		 <property name="qq" column="Es_QQ"></property>
		 <property name="weixin" column="Es_WeiXin"></property>
		 <property name="resume" column="Es_Resume"></property>
		 <property name="exPhoto" column="Es_ExPhoto"></property>
		 <property name="jobTitle" column="Es_JobTitle"></property>
		 <property name="workPlace" column="Es_WorkPlace"></property>
		 <property name="province" column="Es_Province"></property>
		 <property name="city" column="Es_City"></property>
		 <property name="county" column="Es_County"></property>
		 <property name="expertise" column="Es_Expertise"></property>
