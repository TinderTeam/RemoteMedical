<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
	
<hibernate-mapping
	package="cn.fuego.remote.medical.domain">
	<class name="Expert" table="expert">

	    <id name="id" column="Es_ID" length="64">
			 
		</id>

		 <property name="name" column="Es_Name" length="64"></property>
		 <property name="sex" column="Es_Sex" length="4"></property>
		 <property name="age" column="Es_Age" length="16"></property>
		 <property name="phoneNo" column="Es_PhoneNO" length="16"></property>
		 <property name="telephoneNo" column="Es_TelephoneNO" length="32"></property>
		 <property name="email" column="Es_Email" length="64"></property>
		 <property name="qq" column="Es_QQ" length="16"></property>
		 <property name="weixin" column="Es_WeiXin" length="32"></property>
		 <property name="resume"  >
		    <column name="Es_Resume" sql-type="text" ></column>
		 </property>
		 <property name="exPhoto"  >
		  <column name="Eb_ExPhoto"  sql-type="blob"></column>
		 </property>
		 <property name="jobTitle" column="Es_JobTitle" length="32"></property>
		 <property name="workPlace" column="Es_WorkPlace" length="128"></property>
		 <property name="province" column="Es_Province" length="32"></property>
		 <property name="city" column="Es_City" length="32"></property>
		 <property name="county" column="Es_County" length="32"></property>
		 <property name="expertise" column="Es_Expertise" length="128"> </property>
		 
		 <property name="remark"   >
		    <column name="Es_Remark"  sql-type="text"></column>
		 </property>
		 <property name="signName" >
		    <column name="Eb_SignName"  sql-type="blob"></column>
		 </property>
		 
		 <property name="authority" >
		   <column name="Ei_Authority" sql-type="smallint"></column>
		 </property>
		 <property name="state" >
		    <column name="Ei_State" sql-type="smallint"></column>
		 </property>
		 
		 <property name="onlineState">
		    <column name="Ei_OnlineState" sql-type="smallint"></column>
		 </property>
		 <property name="login" column="Ed_Login"></property>
		 <property name="apply" column="Ed_Apply"></property>
		 <property name="reg" column="Ed_Reg"></property>
		 <property name="auditor" column="Es_Auditor"  length="64" ></property>
		 <property name="ctCount" >
		    <column name="Ei_CTCount" sql-type="smallint"></column>
		 </property>
		 <property name="drCount"  >
		 	<column name="Ei_DRCount" sql-type="smallint"></column>
		 </property>	
		 <property name="mrCount"  >
		 	 <column name="Ei_MRCount" sql-type="smallint"></column>
		 </property>
 
		 <property name="reserve1" column="Es_Reserve1"  length="64" ></property>	 	 
	     <property name="reserve2" column="Es_Reserve2"  length="128" ></property>
		 <property name="reserve3" column="Ei_Reserve3"  > </property>
		 <property name="reserve4" >
		    <column name="Ei_Reserve4" sql-type="smallint"></column>
		 </property>
 


	</class>
</hibernate-mapping>