<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
	
<hibernate-mapping
	package="cn.fuego.remote.medical.domain">
	<class name="Link" table="link">

		<composite-id>
	       <key-property name="hospitalID" column="Hs_ID" length="64" ></key-property>
           <key-property name="expertID" column="Es_ID" length="64" ></key-property>
	    </composite-id>

		 <property name="linkTime" column="LinkTime"></property>
		 <property name="linkState" column="LinkState"></property>


	</class>
</hibernate-mapping>